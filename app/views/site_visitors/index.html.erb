<p id="notice"><%= notice %></p>

<h1>Site Visitors</h1>

<table>
  <thead>
    <tr>
      <th>name</th>
      <th>Phonenumber</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @site_visitors.each do |site_visitor| %>
      <tr>
        <td><%= site_visitor.name %></td>
        <td><%= site_visitor.phonenumber %></td>
        <td><%= link_to 'Show', site_visitor %></td>
        <td><%= link_to 'Edit', edit_site_visitor_path(site_visitor) %></td>
        <td><%= link_to 'Destroy', site_visitor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Site Visitor', new_site_visitor_path %>
